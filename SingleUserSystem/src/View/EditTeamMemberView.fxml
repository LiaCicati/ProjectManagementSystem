<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Edit team member">
         <font>
            <Font size="30.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="426.0" prefWidth="418.0" spacing="40.0">
         <VBox.margin>
            <Insets top="60.0" />
         </VBox.margin>
         <children>
            <HBox prefHeight="78.0" prefWidth="418.0" spacing="30.0">
               <children>
                  <Label text="Name">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <VBox prefHeight="78.0" prefWidth="164.0">
                     <children>
                        <TextField prefHeight="26.0" prefWidth="115.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="45.0">
               <children>
                  <Label text="Role">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <ComboBox prefWidth="150.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" maxWidth="-Infinity" prefHeight="94.0" prefWidth="700.0" spacing="36.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="46.0" prefWidth="134.0" text="Save">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="46.0" prefWidth="134.0" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Label text="Label error" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
   </children>
   <padding>
      <Insets left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
